{
  "env": {
    "node": true,
    "es2021": true,
    "browser": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "globals": {
    "AppTrabajoSDK": "readonly",
    "console": "readonly",
    "module": "readonly",
    "require": "readonly",
    "window": "readonly",
    "Blob": "readonly",
    "setTimeout": "readonly",
    "Promise": "readonly",
    "Date": "readonly",
    "Math": "readonly",
    "Error": "readonly",
    "Object": "readonly",
    "Array": "readonly",
    "String": "readonly",
    "Number": "readonly",
    "Boolean": "readonly"
  },
  "rules": {
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-console": "off",
    "no-unused-expressions": "off",
    "spaced-comment": "off",
    "no-throw-literal": "off",
    "no-case-declarations": "off",
    "complexity": "off"
  }
}
