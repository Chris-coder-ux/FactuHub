    "test:performance": "npx artillery run artillery-verifactu.yml --output report.json",
    "analyze": "ANALYZE=true npm run build",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:fix": "npm audit fix",
    "security:audit:json": "npm audit --audit-level=moderate --json > security-audit-results.json",
