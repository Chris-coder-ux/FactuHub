  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "node --max-old-space-size=512 ./node_modules/.bin/jest",
    "test:watch": "node --max-old-space-size=512 ./node_modules/.bin/jest --watch",
    "test:ocr-accuracy": "tsx scripts/test-ocr-accuracy.ts",
